<%= form_for @cabin do |f| %>
    <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: "form-control" %>
    </div>
    <div class="form-group">
        <%= f.label :bedrooms %>
        <%= f.text_field :bedrooms, class: "form-control" %>
    </div>
    <div class="form-group">
        <%= f.file_field :image, class: "form-control" %>
    </div>
    <div class="form-group">
        <%= f.submit class: "btn btn-primary mb-2"%>
    </div>
<% end %>